  "scripts": {
    "dev": "next dev -p 80",
    "export": "next build && next export",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "ghpages:preBuild": "git checkout gh-pages && git pull && git checkout main",
    "ghpages:push": "git subtree push --prefix out origin gh-pages && git push",
    "ghpages:postBuild": "touch out/.nojekyll && git add out/ && git commit -m \"Deploy gh-pages\" && npm run ghpages:push",
    "ghpages": " npm run ghpages:preBuild && npm run export && npm run ghpages:postBuild",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:clear": "jest --clearCache"
  },
